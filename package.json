{
  "name": "@auspices/eos",
  "version": "0.0.0-development",
  "main": "./dist/cjs/client.js",
  "module": "./dist/esm/client.js",
  "types": "./dist/esm/client.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/client.d.ts",
      "import": "./dist/esm/client.js",
      "require": "./dist/cjs/client.js",
      "default": "./dist/esm/client.js"
    },
    "./client": {
      "types": "./dist/esm/client.d.ts",
      "import": "./dist/esm/client.js",
      "require": "./dist/cjs/client.js",
      "default": "./dist/esm/client.js"
    },
    "./server": {
      "types": "./dist/esm/server.d.ts",
      "import": "./dist/esm/server.js",
      "require": "./dist/cjs/server.js",
      "default": "./dist/esm/server.js"
    },
    "./theme": {
      "types": "./dist/esm/theme/index.d.ts",
      "import": "./dist/esm/theme/index.js",
      "require": "./dist/cjs/theme/index.js",
      "default": "./dist/esm/theme/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "examples/next-minimal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/auspices/eos.git"
  },
  "author": "@dzucconi",
  "license": "MIT",
  "scripts": {
    "build:storybook": "storybook build",
    "build:cjs": "tsc -p tsconfig.build.cjs.json",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "clean": "rm -rf dist",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src --ext .tsx,.ts --fix",
    "format": "prettier --write '**/*'",
    "prepublishOnly": "yarn build",
    "semantic-release": "semantic-release",
    "start": "yarn storybook",
    "storybook": "storybook dev -p 6006",
    "type:check": "tsc -p tsconfig.json --noEmit",
    "verify:release": "yarn type:check && yarn lint && yarn build && yarn workspace eos-next-minimal build",
    "unlink-all": "yalc remove --all && yarn --check-files"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styled-components": ">=5.0.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@seznam/compose-react-refs": "^1.0.6",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "@types/styled-system": "5.1.23",
    "@types/styled-system__theme-get": "5.0.4",
    "proportional-scale": "^4.0.0",
    "react-focus-on": "^3.9.4",
    "styled-reset": "^4.5.2",
    "styled-system": "^5.1.5",
    "use-keyboard-list-navigation": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@storybook/addon-actions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-viewport": "^8.6.4",
    "@storybook/addon-webpack5-compiler-swc": "^2.1.0",
    "@storybook/react": "^8.6.4",
    "@storybook/react-webpack5": "^8.6.4",
    "@types/node": "22.13.9",
    "@types/react-dom": "19.0.4",
    "@types/styled-components": "5.1.34",
    "@types/styled-system__should-forward-prop": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "babel-loader": "10.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.21.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "prettier": "3.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "semantic-release": "25.0.3",
    "storybook": "^8.6.4",
    "storybook-states": "1.2.0",
    "styled-components": "6.1.15",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "labels": false
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@4.6.0"
}
